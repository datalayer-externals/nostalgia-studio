{
  "compilerOptions": {
    "module": "esnext",
    "target": "es5",
    "allowJs": false,
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "checkJs": false,
    "lib": ["dom", "es2015", "es2016", "es2017", "esnext"],
    "experimentalDecorators": false,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@dnd-kit/core": ["utils/dnd-kit/packages/core/src"],
      "@dnd-kit/accessibility": ["utils/dnd-kit/packages/accessibility/src"],
      "@dnd-kit/modifiers": ["utils/dnd-kit/packages/modifiers/src"],
      "@dnd-kit/sortable": ["utils/dnd-kit/packages/sortable/src"],
      "@dnd-kit/utilities": ["utils/dnd-kit/packages/utilities/src"],
      "@floating-ui/core": ["utils/floating-ui/core/src/"],
      "@floating-ui/dom": ["utils/floating-ui/dom/src/"],
      "@floating-ui/react-dom": ["utils/floating-ui/reacat-dom/src/"],
      "@floating-ui/react-dom-interactions": [
        "utils/floating-ui/reacat-dom-interactions/src/"
      ],
      "slate": ["editor-slate/src-pkgs/slate/src"],
      "slate-history": ["editor-slate/src-pkgs/slate-history/src"],
      "slate-react": ["editor-slate/src-pkgs/slate-react/src"],
      "slate-hyperscript": ["editor-slate/src-pkgs/slate-hyperscript/src"]
    }
  },
  "files": ["typings.d.ts"]
}
