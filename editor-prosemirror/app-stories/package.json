{
  "name": "@examples-hub/prosemirror-app-stories",
  "version": "1.0.0111",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "cross-env APP_ENV=reacthot webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo2": "run-p dev:collab-basic-frontend dev:collab-basic-server",
    "dev:collab-basic-frontend": "cross-env APP_ENV=reacthot webpack --watch --hot --config webpack/webpack.config.demo.js",
    "dev:collab-basic-server2": "ts-node-dev --transpile-only ./src/prosemirror-collab-basic/run.ts",
    "dev:collab-basic-server": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm/transpile-only ./src/prosemirror-collab-basic/run.ts",
    "start": "npm run demo",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "3.0.0111",
    "@dnd-kit/core": "5.0.3111",
    "@dnd-kit/modifiers": "5.0.0111",
    "@dnd-kit/sortable": "6.0.1111",
    "@dnd-kit/utilities": "3.1.0111",
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.7.1",
    "@faker-js/faker": "^7.5.0",
    "classnames": "^2.3.1",
    "crelt": "1.0.5",
    "is-hotkey": "^0.1.6",
    "is-url": "^1.2.2",
    "prosemirror-collab": "1.3.0111",
    "prosemirror-commands": "1.3.0111",
    "prosemirror-dropcursor": "1.6.0111",
    "prosemirror-example-setup": "1.2.1111",
    "prosemirror-gapcursor": "1.3.1111",
    "prosemirror-history": "1.3.0111",
    "prosemirror-inputrules": "1.2.0111",
    "prosemirror-keymap": "1.2.0111",
    "prosemirror-markdown": "1.9.4111",
    "prosemirror-menu": "1.2.1111",
    "prosemirror-model": "1.18.1111",
    "prosemirror-schema-basic": "1.2.0111",
    "prosemirror-schema-list": "1.2.1111",
    "prosemirror-state": "1.4.1111",
    "prosemirror-tables": "1.2.5111",
    "prosemirror-test-builder": "1.1.0111",
    "prosemirror-transform": "1.7.0111",
    "prosemirror-view": "1.27.2111",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/is-url": "^1.2.30",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.9",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "prosemirror-dev-toolkit": "^1.0.2",
    "prosemirror-dev-tools": "^3.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
