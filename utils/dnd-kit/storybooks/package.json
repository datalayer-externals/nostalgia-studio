  "license": "MIT",
  "scripts": {
    "lerna": "lerna",
    "start": "lerna run start --stream --parallel",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "lerna run build",
    "build:storybook": "build-storybook",
    "prepublish": "lerna run prepublish",
    "start:playground": "yarn run build && yarn --cwd playground && yarn --cwd playground start",
    "start1": "start-storybook -p 6006 -s ./.storybook/assets",
    "start:storybook": "start-storybook -p 6006 -s ./.storybook/assets",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "changeset": "changeset",
    "release": "changeset publish",
    "version:next": "changeset version --snapshot next",
    "release:next": "changeset publish --tag next"
  },
  "dependencies": {
    "@dnd-kit/accessibility": "3.0.1111",
    "@dnd-kit/core": "6.0.8111",
    "@dnd-kit/modifiers": "6.0.1111",
    "@dnd-kit/sortable": "7.0.2111",
    "@dnd-kit/utilities": "3.2.1111"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.3.5",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.5",
    "@storybook/addons": "^6.3.5",
    "@storybook/react": "^6.3.5",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "classnames": "^2.3.2",
    "css-loader": "^6.7.3",
    "framer-motion": "^6.2.8",
    "postcss": "^8.4.14",
    "postcss-loader": "^4.1.0",
    "postcss-nested": "^5.0.1",
    "postcss-simple-vars": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tiny-virtual-list": "^2.2.0",
    "style-loader": "^3.3.2",
    "tsdx": "^0.14.1"
  }
}
