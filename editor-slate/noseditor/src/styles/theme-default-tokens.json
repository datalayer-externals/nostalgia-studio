  "color": {
    "$type": "color",
    "brand": {
      "primary": {
        "$value": "{palette.teal500}"
      },
      "light": {
        "$value": "{palette.teal200}"
      },
      "lighter": {
        "$value": "{palette.teal50}"
      },
      "dark": {
        "$value": "{palette.teal600}"
      },
      "darker": {
        "$value": "{palette.teal800}"
      }
    },
    "background": {
      "default": {
        "$value": "{palette.gray25}"
      },
      "hover": {
        "$value": "{palette.gray50}"
      }
    },
    "text": {
      "body": {
        "$value": "{palette.black}"
      },
      "muted": {
        "$value": "{palette.grey500}"
      },
      "error": {
        "$value": "{palette.red700}"
      },
      "link": {
        "$value": "{palette.blue700}"
      }
    },
    "border": {
      "light": {
        "$value": "{palette.gray300}"
      },
      "muted": {
        "$value": "{palette.grey100}"
      }
    }
  },
  "spacing": {
    "$type": "dimension",
    "spacer0": {
      "$value": "0px"
    },
    "spacerXs": {
      "$value": "4px"
    },
    "spacerS": {
      "$value": "8px"
    },
    "spacerM": {
      "$value": "16px"
    },
    "spacerX": {
      "$value": "24px"
    },
    "spacerXl": {
      "$value": "32px"
    },
    "spacerXxl": {
      "$value": "40px"
    }
  },
  "size": {
    "$type": "dimension",
    "borderRadius": {
      "xs": {
        "$value": "2px"
      },
      "sm": {
        "$value": "4px"
      },
      "md": {
        "$value": "6px"
      },
      "lg": {
        "$value": "8px"
      },
      "xl": {
        "$value": "10px"
      }
    }
  },
  "shadow": {
    "$type": "shadow",
    "sm": {
      "$value": {
        "offsetX": "0px",
        "offsetY": "8px",
        "blur": "16px",
        "spread": "0px",
        "color": "rgba(100, 100, 111, 0.2)"
      }
    },
    "md": {
      "$value": {
        "offsetX": "0px",
        "offsetY": "12px",
        "blur": "24px",
        "spread": "0px",
        "color": "rgba(100, 100, 111, 0.2)"
      }
    },
    "lg": {
      "$value": {
        "offsetX": "0px",
        "offsetY": "16px",
        "blur": "30px",
        "spread": "0px",
        "color": "rgba(100, 100, 111, 0.2)"
      }
    }
  },
  "font": {
    "family": {
      "$type": "font",
      "system": {
        "$value": [
          "-apple-system,",
          "BlinkMacSystemFont",
          "Segoe UI",
          "Helvetica",
          "Arial",
          "sans-serif",
          "Apple Color Emoji",
          "Segoe UI Emoji"
        ]
      },
      "monospace": {
        "$value": [
          "SFMono-Regular",
          "Consolas",
          "Liberation Mono",
          "Menlo",
          "Courier",
          "monospace"
        ]
      },
      "display": {
        "$value": [
          "InterUI",
          "-apple-system",
          "BlinkMacSystemFont",
          "Helvetica Neue",
          "Segoe UI",
          "Oxygen",
          "Ubuntu",
          "Cantarell",
          "Open Sans",
          "sans-serif"
        ]
      },
      "size": {
        "$type": "dimension",
        "$extensions": {
          "modes": [
            "mobile",
            "desktop"
          ]
        },
        "f00": {
          "$value": "32px",
          "$extensions": {
            "mode": {
              "mobile": "32px",
              "desktop": "48px"
            }
          }
        },
        "f0": {
          "$value": "32px",
          "$extensions": {
            "mode": {
              "mobile": "32px",
              "desktop": "40px"
            }
          }
        },
        "f1": {
          "$value": "26px",
          "$extensions": {
            "mode": {
              "mobile": "26px",
              "desktop": "32px"
            }
          }
        },
        "f2": {
          "$value": "22px",
          "$extensions": {
            "mode": {
              "mobile": "22px",
              "desktop": "24px"
            }
          }
        },
        "f3": {
          "$value": "16px",
          "$extensions": {
            "mode": {
              "mobile": "16px",
              "desktop": "20px"
            }
          }
        },
        "f4": {
          "$value": "16px",
          "$extensions": {
            "mode": {
              "mobile": "16px",
              "desktop": "16px"
            }
          }
        },
        "f5": {
          "$value": "14px",
          "$extensions": {
            "mode": {
              "mobile": "14px",
              "desktop": "14px"
            }
          }
        },
        "f6": {
          "$value": "12px",
          "$extensions": {
            "mode": {
              "mobile": "12px",
              "desktop": "12px"
            }
          }
        }
      },
      "lineHeight": {
        "$type": "dimension",
        "xs": {
          "$value": "1"
        },
        "sm": {
          "$value": "1.25"
        },
        "md": {
          "$value": "1.5"
        },
        "default": {
          "$value": "{font.family.lineHeight.md}"
        },
        "lg": {
          "$value": "1.8"
        },
        "xl": {
          "$value": "2"
        }
      }
    }
  }
}
