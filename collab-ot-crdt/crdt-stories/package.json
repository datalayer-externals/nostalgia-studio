{
  "name": "@examples-hub/crdt-tories",
  "version": "1.1.1111",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "scripts": {
    "build": "rimraf dist && npm run build:prod",
    "build:babel": "rimraf dist && BABEL_ENV=build babel src --out-dir dist --root-mode upward --ignore 'src/**/*.test.js','src/**/*.stories.js' --source-maps --verbose",
    "build:prod": "rimraf dist && cross-env NODE_ENV=production webpack --config webpack/webpack.config.prod.js",
    "build:types": "",
    "demo": "webpack serve --hot --config webpack/webpack.config.demo.cjs",
    "demo:idbsidesync": "webpack serve --hot --config webpack/webpack.config.idbsync.cjs",
    "demo:crdt-hlc": "serve ./crdt-example-app-hlc-jlongster",
    "demo:gdrive": "cd public && serve -C -l 8999",
    "start": "npm run demo:idbsidesync",
    "server:crdt-hlc": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./crdt-example-app-hlc-jlongster/server/index.js",
    "server:crdt-idbsidesync": "node --experimental-specifier-resolution=node --loader ts-node/esm/transpile-only ./idb-side-sync/server/index.ts",
    "server": "npm run server:crdt-hlc",
    "test": "jest",
    "test:log": "node index.js --name nested-a"
  },
  "dependencies": {
    "better-sqlite3": "^7.6.2",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "murmurhash": "^0.0.2",
    "polka": "^0.5.2",
    "serve": "^14.0.1",
    "sirv": "^2.0.2",
    "uuid": "^9.0.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.2",
    "@types/ws": "^8.5.3",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.1"
  }
}
