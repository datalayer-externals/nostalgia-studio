{
  "name": "@datalking/nostalgia-studio-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "markdown-based, easy-to-read, easy-to-write and content-centric web app for data and ideas.",
  "keywords": [
    "markdown"
  ],
  "author": "jinyaoo <jinyaoo@qq.com>",
  "license": "MIT",
  "workspaces": [
    "site/*",
    "examples/*",
    "editor-prosemirror/*",
    "editor-prosemirror/src-pkgs/*",
    "editor-prosemirror/teemu-pkgs/*",
    "editor-slate/*",
    "editor-slate/src-pkgs/*",
    "excel-tanstack-table/*",
    "excel-tanstack-table/src-pkgs/*",
    "collab-yjs/*",
    "collab-yjs/src-pkgs/*",
    "collab-ot-crdt/*",
    "collab-ot-crdt/src-ottypes/*",
    "store-nedb",
    "utils/*",
    "utils/dnd-kit/packages/*",
    "utils/dnd-kit/playground",
    "utils/dnd-kit/storybooks",
    "utils/floating-ui/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "rimraf \"packages/**/dist\"",
    "dev": "",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json,cjs,mjs}\" --write",
    "format:check": "npm run formatter -- --check",
    "help": "echo 'exec npm start to view the demo'",
    "preinstall": "",
    "lint": "eslint --fix \"store-nedb/**/*.{js,jsx,ts,tsx,cjs,mjs}\"",
    "lint:js": "",
    "lint:style": "",
    "prepare": "",
    "publish": "",
    "start": "npm run start:editor-pme",
    "start:dashboard": "cd packages/nostalgia-dashboard && npm run demo",
    "start:editor-nostalgia": "cd packages/nostalgia-editor && npm run site",
    "start:editor-pme": "cd editor-prosemirror/app-stories && npm run start",
    "start:editor-slate": "cd editor-slate/app-stories && npm run start",
    "start:materials-text-editor": "cd packages/materials-text-editor && npm run demo",
    "syncpack-fix": "syncpack-fix-mismatches",
    "syncpack-ls": "syncpack-list-mismatches",
    "test": "jest --config=jest.monorepo.js",
    "test-bug-docs": "npm x -w @examples-hub/sample-eg-react-comp-docs -- npm run dev",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "not IE_Mob 11"
  ],
  "dependencies": {
    "@atao60/fse-cli": "^0.1.7",
    "classnames": "^2.3.2",
    "cross-fetch": "^3.1.5",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "lodash": "^4.17.21",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tinycolor2": "^1.4.2",
    "uuid": "^8.0.0"
  },
  "devDependencies": {
    "@adobe/react-spectrum": "^3.21.2",
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.1",
    "@babel/plugin-proposal-decorators": "^7.18.6",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
    "@babel/preset-env": "^7.18.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.0",
    "@faker-js/faker": "^7.5.0",
    "@mdx-js/loader": "^2.1.3",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.1",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "babel-jest": "^27.0.5",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.24.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.24.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^27.3.1",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "sass-loader": "^13.0.2",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "syncpack": "^8.2.4",
    "terser-webpack-plugin": "^5.3.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4",
    "circular-dependency-plugin": "^5.2.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "yargs": "^17.5.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.3.0"
  },
  "overrides": {
    "@teemu/client-cra": {}
  }
}
